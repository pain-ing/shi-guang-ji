<svg width="512" height="512" viewBox="0 0 512 512" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#6d49ff;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#a855f7;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="inner" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#ffffff;stop-opacity:0.9" />
      <stop offset="100%" style="stop-color:#ffffff;stop-opacity:0.7" />
    </linearGradient>
  </defs>
  
  <!-- 背景 -->
  <rect width="512" height="512" rx="64" ry="64" fill="url(#bg)" />
  
  <!-- 装饰圆形 -->
  <circle cx="128" cy="128" r="32" fill="url(#inner)" opacity="0.3" />
  <circle cx="384" cy="384" r="48" fill="url(#inner)" opacity="0.2" />
  
  <!-- 主图标 - 时光符号 -->
  <g transform="translate(256,256)">
    <!-- 外圈 -->
    <circle cx="0" cy="0" r="120" fill="none" stroke="url(#inner)" stroke-width="8" opacity="0.8" />
    
    <!-- 内圈 -->
    <circle cx="0" cy="0" r="80" fill="url(#inner)" opacity="0.9" />
    
    <!-- 时钟指针效果 -->
    <line x1="0" y1="0" x2="0" y2="-60" stroke="#6d49ff" stroke-width="6" stroke-linecap="round" />
    <line x1="0" y1="0" x2="40" y2="0" stroke="#6d49ff" stroke-width="4" stroke-linecap="round" />
    
    <!-- 中心点 -->
    <circle cx="0" cy="0" r="8" fill="#6d49ff" />
    
    <!-- 装饰点 -->
    <circle cx="0" cy="-100" r="4" fill="url(#inner)" />
    <circle cx="100" cy="0" r="4" fill="url(#inner)" />
    <circle cx="0" cy="100" r="4" fill="url(#inner)" />
    <circle cx="-100" cy="0" r="4" fill="url(#inner)" />
  </g>
</svg>
